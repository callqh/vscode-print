{
  "name": "js-rust-print",
  "displayName": "一键打印Js和Rust中的选中变量",
  "description": "一个可以快速打印选中变量的插件",
  "version": "0.0.1",
  "keywords": [
    "Rust",
    "JavaScript",
    "console",
    "print"
  ],
  "publisher": "liuqh",
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other",
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:console"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "console",
        "title": "rust print"
      }
    ],
    "keybindings": [
      {
        "command": "console",
        "key": "ctrl+shift+l",
        "mac": "cmd+shift+l",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.62.0",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4",
    "ts-loader": "^9.2.5",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0",
    "@vscode/test-electron": "^1.6.2"
  }
}
